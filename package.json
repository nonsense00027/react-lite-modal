{
  "name": "react-lite-modal",
  "version": "1.0.0",
  "description": "lite and bare modal for react",
  "author": "m1lagro",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "dependencies": {
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "publish": "npm publish --access public",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major"
  },
  "keywords": [
    "react",
    "hooks",
    "custom hooks"
  ]
}
